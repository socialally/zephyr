{
  "name": "zephyr",
  "version": "1.3.4",
  "description": "Plugin facade",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "git://github.com/tmpfs/zephyr.git"
  },
  "main": "lib/zephyr.js",
  "devDependencies": {
    "browser-sync": "~2.4.0",
    "browserify": "~9.0.3",
    "chai": "~2.1.0",
    "coveralls": "~2.11.6",
    "del": "~1.1.1",
    "emanate": "~1.2.1",
    "es5-shim": "~4.1.0",
    "gulp": "~3.8.11",
    "gulp-eslint": "~0.5.0",
    "gulp-istanbul": "~0.6.0",
    "gulp-istanbul-report": "0.0.1",
    "gulp-mocha": "~2.0.0",
    "gulp-mocha-phantomjs": "~0.8.1",
    "gulp-size": "~1.2.1",
    "gulp-sourcemaps": "~1.4.0",
    "gulp-uglify": "~1.1.0",
    "gulp-util": "~3.0.4",
    "istanbul": "~0.3.6",
    "mocha": "~2.1.0",
    "mocha-phantomjs-istanbul": "0.0.2",
    "require-dir": "~0.1.0",
    "run-sequence": "~1.0.2",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.0.0"
  },
  "scripts": {
    "docs": "npm run readme",
    "readme": "mdp --force -v",
    "start": "gulp start",
    "lint": "gulp lint",
    "clean": "gulp clean",
    "spec": "gulp spec",
    "instrument": "istanbul instrument --output instrument lib",
    "cover": "npm run instrument && gulp cover",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "gulp test"
  },
  "mdp": {
    "title": "Zephyr",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/tmpfs/zephyr",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "usage.md",
          "plugins.md"
        ]
      },
      {
        "title": "Source",
        "inc": "../../lib/zephyr.js",
        "type": "code",
        "language": "javascript"
      },
      {
        "inc": [
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
